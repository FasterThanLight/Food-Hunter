<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.muhamadaizuddin.foodhunter.MainActivity"
    android:background="#00000000">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:background="#00ffffff"
        android:paddingEnd="0dp"
        android:baselineAligned="false"
        android:id="@+id/linearLayout">

        <ImageView
            android:layout_width="36dp"
            android:layout_height="31dp"
            android:id="@+id/imageView"
            android:soundEffectsEnabled="true"
            android:src="@android:drawable/ic_menu_mylocation"
            android:layout_gravity="center_vertical"
            android:paddingEnd="0dp"
            android:paddingStart="0dp"
            android:background="#00000000" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Besut, Terengganu"
            android:id="@+id/textView"
            android:layout_weight="0.98"
            android:textColor="#000000"
            android:textSize="22dp"
            android:password="false"
            android:phoneNumber="false"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textAlignment="center"
            android:background="#bdbdbd"
            android:autoText="false" />

        <Button
            android:layout_width="25dp"
            android:layout_height="fill_parent"
            android:id="@+id/button2"
            android:background="#00000000"
            android:singleLine="false"
            android:layout_weight="0.03"
            android:foreground="@android:drawable/ic_menu_search" />

    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/linearLayout"
        android:layout_alignStart="@+id/linearLayout"
        android:paddingTop="10dp"
        android:background="#ffffff">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#00000000"
            android:baselineAligned="false">

            <ImageButton
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:id="@+id/imageButton2"
                android:src="@drawable/cafe"
                android:scaleType="fitXY"
                android:background="#00000000"
                android:padding="10dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="#00000000">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="CAFE RACER"
                    android:id="@+id/textView3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="3.00 p.m-12.00 a.m"
                    android:id="@+id/textView2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="019-9897654"
                    android:id="@+id/textView4" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#00000000">

            <ImageButton
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:id="@+id/imageButton3"
                android:src="@drawable/peranakan"
                android:scaleType="fitXY"
                android:background="#00000000"
                android:padding="10dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="#00000000">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Peranakan Place"
                    android:id="@+id/textView6"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="10.00 a.m-10.00 p.m"
                    android:id="@+id/textView7"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="017-5432098"
                    android:id="@+id/textView8"
                    android:background="#00000000" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#00000000">

            <ImageButton
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:id="@+id/imageButton4"
                android:src="@drawable/ib"
                android:scaleType="fitXY"
                android:background="#00000000"
                android:padding="10dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="#00000000">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Restoran Ikan Bakar"
                    android:id="@+id/textView9"
                    android:textSize="20dp"
                    android:textIsSelectable="false"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="5.00 p.m-12.00 p.m"
                    android:id="@+id/textView10"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="016-2536478"
                    android:id="@+id/textView11"
                    android:background="#00000000" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#00000000">

            <ImageButton
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:id="@+id/imageButton5"
                android:src="@drawable/fisherman"
                android:scaleType="fitXY"
                android:background="#00000000"
                android:padding="10dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="#00000000">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Fisherman&apos;s Grill"
                    android:id="@+id/textView12"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="8.00 p.m-12.00 p.m"
                    android:id="@+id/textView13"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="012-3626278"
                    android:id="@+id/textView14"
                    android:background="#00000000" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="#00000000">

            <ImageButton
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:id="@+id/imageButton6"
                android:src="@drawable/upin"
                android:scaleType="fitXY"
                android:background="#00000000"
                android:padding="10dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="#00000000">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Restoran Upin &amp; Ipin"
                    android:id="@+id/textView15"
                    android:textSize="20dp"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="10.00 a.m-10.00 p.m"
                    android:id="@+id/textView16"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="017-2736456"
                    android:id="@+id/textView17"
                    android:background="#00000000" />

            </LinearLayout>

        </LinearLayout>

    </TableLayout>
</RelativeLayout>